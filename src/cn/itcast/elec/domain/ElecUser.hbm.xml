<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.elec.domain.ElecUser" table="Elec_User">
		<id name="userID" type="string">
			<column name="userID"  not-null="true" />
			<generator class="uuid"/>
		</id>
		<property name="jctID" type="string">
			<column name="jctID"/>
		</property>
		<property name="userName" type="string">
			<column name="userName"/>
		</property>
		<property name="loginName" type="string">
			<column name="loginName"/>
		</property>
		<property name="loginPassword" type="string">
			<column name="loginPassword"/>
		</property>
		<property name="sexID" type="string">
			<column name="sexID"/>
		</property>
		<property name="birthday" type="date">
			<column name="birthday" length="50"/>
		</property>
		<property name="address" type="string">
			<column name="address"/>
		</property>
		<property name="contactTel" type="string">
			<column name="contactTel"/>
		</property>
		<property name="email" type="string">
			<column name="email"/>
		</property>
		<property name="mobile" type="string">
			<column name="mobile"/>
		</property>
		<property name="isDuty" type="string">
			<column name="isDuty"/>
		</property>
		<property name="onDutyDate" type="date">
			<column name="onDutyDate" length="50"/>
		</property>
		<property name="offDutyDate" type="date">
			<column name="offDutyDate" length="50"/>
		</property>
		<property name="remark" type="string">
			<column name="remark"/>
		</property>
	</class>
</hibernate-mapping>